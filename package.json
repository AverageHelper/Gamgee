{
	"name": "gamgee",
	"version": "1.7.0",
	"description": "A Discord bot for managing a song request queue.",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=development ./node_modules/.bin/nodemon --inspect dist/main.js",
		"setup": "npm ci && npm run build:clean && npm run migrate:only && npm run commands:deploy",
		"commands:deploy": "node . --deploy-commands",
		"commands:revoke": "node . --revoke-commands",
		"premigrate": "npm run build:only",
		"migrate": "npm run migrate:only",
		"migrate:only": "./node_modules/.bin/typeorm migration:run -d ./dist/database/sqliteDataSource.js",
		"lint": "npm run export-version && ./node_modules/.bin/eslint . --ext .ts",
		"lint:fix": "npm run lint -- --fix",
		"prebuild": "npm run lint",
		"build": "npm run build:only",
		"build:only": "./node_modules/.bin/tsc -p tsconfig.prod.json",
		"build:clean": "rm -rf dist && npm run build",
		"export-version": "./node_modules/.bin/genversion ./src/version.ts -esd",
		"test": "npm run test:src && npm run test:e2e",
		"test:src": "./node_modules/.bin/jest",
		"test:src:watch": "npm run test:src -- --watch --coverage=false",
		"test:e2e": "./node_modules/.bin/concurrently -kn bot,mocha -s first 'npm run start:test' 'sleep 3 && npm run _test:e2e'",
		"start:test": "DATABASE_FOLDER=./tests/db-test NODE_ENV=test node .",
		"_test:e2e": "TS_NODE_PROJECT='./tsconfig.e2e.json' ./node_modules/.bin/mocha"
	},
	"main": "./dist/main.js",
	"type": "commonjs",
	"engines": {
		"node": ">=16.10"
	},
	"files": [
		"dist/"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AverageHelper/Gamgee.git"
	},
	"author": "AverageHelper",
	"license": "LICENSE",
	"bugs": {
		"url": "https://github.com/AverageHelper/Gamgee/issues"
	},
	"homepage": "https://github.com/AverageHelper/Gamgee#readme",
	"dependencies": {
		"@averagehelper/job-queue": "1.0.1",
		"cheerio": "1.0.0-rc.12",
		"cross-fetch": "3.1.5",
		"discord.js": "14.1.2",
		"dotenv": "16.0.1",
		"humanize-duration": "3.27.2",
		"lodash": "4.17.21",
		"node-persist": "3.1.0",
		"reflect-metadata": "0.1.13",
		"soundcloud-scraper": "5.0.3",
		"source-map-support": "0.5.21",
		"sqlite3": "5.0.5",
		"typeorm": "0.3.7",
		"winston": "3.8.1",
		"yargs": "17.5.1",
		"ytdl-core": "4.11.0"
	},
	"devDependencies": {
		"@types/chai": "4.3.3",
		"@types/humanize-duration": "3.27.1",
		"@types/jest": "28.1.6",
		"@types/lodash": "4.14.182",
		"@types/mocha": "9.1.1",
		"@types/node": "16.9.6",
		"@types/node-persist": "3.1.2",
		"@types/source-map-support": "0.5.4",
		"@typescript-eslint/eslint-plugin": "5.31.0",
		"@typescript-eslint/parser": "5.31.0",
		"chai": "4.3.6",
		"concurrently": "7.2.2",
		"eslint": "8.20.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-strictest": "0.4.0",
		"eslint-plugin-deprecation": "1.3.2",
		"eslint-plugin-jest": "26.6.0",
		"eslint-plugin-mocha": "10.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-unicorn": "43.0.2",
		"genversion": "3.1.1",
		"jest": "28.1.3",
		"jest-environment-node": "28.1.3",
		"jest-extended": "3.0.1",
		"mocha": "10.0.0",
		"nodemon": "2.0.19",
		"prettier": "2.7.1",
		"ts-jest": "28.0.7",
		"ts-node": "10.9.1",
		"type-fest": "2.18.0",
		"typescript": "4.7.4"
	}
}
